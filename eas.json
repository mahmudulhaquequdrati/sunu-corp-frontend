{
  "cli": {
    "version": ">= 0.48.2",
    "requireCommit": true
  },
  "build": {
    "production": {
      "yarn": "1.22.18",
      "node": "16.14.0",
      "distribution": "store",
      "android": {
        "image": "latest",
        "buildType": "app-bundle"
      },
      "ios": {
        "image": "latest",
        "cocoapods": "1.11.2",
        "buildConfiguration": "Release"
      }
    },
    "development": {
      "extends": "production",
      "distribution": "internal",
      "android": {
        "image": "latest",
        "gradleCommand": ":app:assembleRelease",
        "buildType": "apk"
      },
      "ios": {
        "image": "latest",
        "cocoapods": "1.11.2",
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "extends": "production",
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {}
  }
}
